{
  "expo": {
    "name": "Flappy Bird",
    "slug": "flappy-bird",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flappy-bird-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/flappy-bird-splash.png",
