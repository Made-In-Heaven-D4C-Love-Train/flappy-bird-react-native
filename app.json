      "adaptiveIcon": {
        "foregroundImage": "./assets/flappy-bird-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ledieudelaflemme.flappybird"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "63609b95-7f58-4bc2-a0a6-3cd9799919ce"
      }
    }
